<h2 class="title">Photos</h2>

<%= paginate @photos %>
<table class="table is-striped is-fullwidth is-bordered">
  <thead>
    <tr>
      <th><abbr title="Photo ID">Link</abbr></th>
      <th><abbr>Photographer</abbr></th>
    </tr>
  </thead>
  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <th><%= link_to photo.id, photo %></th>
        <td><%= photo.photographer.photographer %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @photos %>